<?xml version="1.0"?>
<block>
  <name>real_value</name>
  <key>inets_real_value</key>
  <category>iNets</category>
  <import>import inets</import>
  <make>inets.real_value($filename, $key)</make>

  <param>
    <name>Filename</name>
    <key>filename</key>
    <type>string</type>
  </param>

  <param>
    <name>Key</name>
    <key>key</key>
    <type>string</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
